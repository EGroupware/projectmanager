<overlay>
	<template id="gantt_print_dialog" template="" lang="" group="0" version="16.1">
		<vbox>
			<description value="Columns to print"/>
			<select id="columns" rows="8" selected_first="false"/>
			<description value="If the gantt chart is wider than the page, it will be trimmed."/>
